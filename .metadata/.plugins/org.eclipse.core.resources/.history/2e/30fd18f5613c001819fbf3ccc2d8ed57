public class EMMain {

	public static void main(String[] args) {
		// 외부 파일에서 불러오기
		BufferedReader br = null;
		try {
			FileReader fr = new FileReader("C:\Users\soldesk\Desktop");
		} catch (Exception e) {
			e.printStackTrace();
		} finally {
			br.close();
		}
	}

}
