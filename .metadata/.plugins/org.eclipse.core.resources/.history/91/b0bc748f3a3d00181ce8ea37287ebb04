<!DOCTYPE mapper
 PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="lhw">
	<select id="getAllProduct" resultType="com.kwon.mb.main.Product">
		select * from
		apr11_product order by p_name
	</select>
	<insert id="reg" parameterType="com.kwon.mb.main.Product">
		insert into apr11_product values(
		apr11_product_seq.nextval,
		#{p_name},
		#{p_img},
		#{p_price},
		#{p_exp}
		)
	</insert>
</mapper>
